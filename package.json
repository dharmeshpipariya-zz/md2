        "es6-shim": "0.33.13",
        "eslint": "1.10.3",
        "exports-loader": "0.6.3",
        "file-loader": "0.8.5",
        "gulp": "3.9.1",
        "gulp-clean": "0.3.2",
        "gulp-eslint": "1.1.1",
        "gulp-size": "2.1.0",
        "gulp-tsc": "1.1.4",
        "gulp-tslint": "4.3.5",
        "html-loader": "0.4.2",
        "markdown-loader": "0.1.7",
        "marked": "0.3.5",
