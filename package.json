        "type": "git",
        "url": "git+ssh://git@github.com/DharmeshPipariya/md2.git"
    },
    "bugs": {
        "url": "https://github.com/DharmeshPipariya/md2/issues"
    },
    "homepage": "https://github.com/DharmeshPipariya/md2#readme",
    "dependencies": { },
    "devDependencies": {
        "angular2": "2.0.0-beta.1",
        "bootstrap": "3.3.6",
